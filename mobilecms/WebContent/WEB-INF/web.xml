 	 </servlet>

<servlet>
    <servlet-name>getPunjabNews</servlet-name>
    <servlet-class>com.itgd.server.getPunjabNews</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PoliticalNews</servlet-name>
    <servlet-class>com.itgd.server.PoliticalNews</servlet-class>
  </servlet>
<servlet>
    <servlet-name>MobilePoliticalNews</servlet-name>
    <servlet-class>com.itgd.server.MobilePoliticalNews</servlet-class>
  </servlet>
<servlet>
    <servlet-name>TestServlet</servlet-name>
    <servlet-class>com.itgd.server.TestServlet</servlet-class>
  </servlet>


<servlet-mapping>
    <servlet-name>TestServlet</servlet-name>
    <url-pattern>/sp/test</url-pattern>
  </servlet-mapping>


<servlet-mapping>
    <servlet-name>MobilePoliticalNews</servlet-name>
    <url-pattern>/sp/polnews</url-pattern>
  </servlet-mapping>

<servlet-mapping>
    <servlet-name>getPunjabNews</servlet-name>
    <url-pattern>/sp/punjabnews</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PoliticalNews</servlet-name>
    <url-pattern>/sp/getpolnews</url-pattern>
  </servlet-mapping>

 	 <servlet-mapping>
    		<servlet-name>PunjabNews</servlet-name>
   		 <url-pattern>/sp/pnews</url-pattern>
 	 </servlet-mapping>
 	<servlet-mapping>
    		<servlet-name>getElectionNewsXml</servlet-name>
    		<url-pattern>/sp/getElectionNewsXml</url-pattern>
 	 </servlet-mapping>

	<servlet-mapping>
   		 <servlet-name>I2iAllHeadlines</servlet-name>
   		 <url-pattern>/sp/i2iall</url-pattern>
  	</servlet-mapping>
  	<servlet-mapping>
    		<servlet-name>getElectionNewsHtml</servlet-name>
   		 <url-pattern>/sp/getElectionNewsHtml</url-pattern>
