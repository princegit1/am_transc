 	 </servlet-mapping>
	<servlet-mapping>
   		 <servlet-name>AirtelUssdHeadlines</servlet-name>
    		<url-pattern>/sp/airtelall</url-pattern>
  	</servlet-mapping>
	

	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
	